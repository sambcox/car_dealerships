<%= form_with url: "/dealerships", method: :post, local: true do |form| %>
  <%= form.label :name %><br>
  <%= form.text_field :name %><br>
  <%= form.label :city %><br>
  <%= form.text_field :city %><br>
  <%= form.label :employees %><br>
  <%= form.number_field :employees %><br>
  <%= form.label :available, "Does this dealership have a service department?" %><br>
  Yes:<%= form.radio_button :service_department, label=true %><br>
  No:<%= form.radio_button :service_department, label=false %><br>
  <%= form.submit 'Create Dealership' %>
<% end %>